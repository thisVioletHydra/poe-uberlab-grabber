  "scripts": {
    "line::main": " ____________ ðŸš€ MAIN ðŸš€ ____________ ",
    "poe": "node ./server/dist/main.js",
    "go": "node-dev ./server/dist/main.js",
    "watch": "del .eslintcache && nuxt",
    "server": "webpack --mode development",
    "prod": "webpack --mode production",
    "fix": "eslint ./src/**/*.js --fix",
    "killnode": "taskkill /f /im node.exe /im RuntimeBroker.exe",
    "line::fetch": " ____________ ðŸš€ FETCH ðŸš€ ____________ ",
    "fetch": "yarn npm-run-all run:fetch::*",
    "run:fetch::fetch": "git fetch",
    "run:fetch::merge": "git merge",
    "run:fetch::yarn": "yarn",
    "line::push": " ____________ ðŸš€ PUSH ðŸš€ ____________ ",
    "push": "yarn npm-run-all run:push::*",
    "run:push::patch": "yarn version --patch",
    "run:push::status": "git status -s",
    "run:push::add": "git add . ",
    "run:push::autopush": "yarn autopush",
    "run:push::push": "git push --follow-tags",
    "line::autopush": " ____________ ðŸš€ AUTOPUSH ðŸš€ ____________ ",
    "autopush": "pwsh -Command . ${env:userprofile}\\.vscode\\scripts\\index.ps1",
    "line::gitfix": " ____________ ðŸš€ GITFIX ðŸš€ ____________ ",
    "gitfixtags": "yarn npm-run-all run:gitfix::*",
    "run::gitfix::removetags": "git tag -d $(git tag -l)",
    "run::gitfix::update": "git fetch",
    "gitfix::hint": "https://stackoverflow.com/a/61204370/8488993",
    "line::build": " ____________ ðŸš€ BUILD ðŸš€ ____________ ",
    "buildDev": "webpack --mode development --watch",
    "buildProd": "webpack --mode production",
    "line::PM2": " ____________ ðŸš€ PM2 ðŸš€ ____________ ",
    "pm2:setup": "pm2 deploy production setup",
    "pm2:build": "nuxt build && yarn prod",
    "pm2:update": "pm2 deploy production update",
    "pm2:preupdate": "yarn buildProd && yarn build",
    "pm2:server": "pm2 startOrRestart ecosystem.config.js --env production && pm2 log group-dashboard-siteanalyzer",
    "line::up": " ____________ ðŸš€ UP ðŸš€ ____________ ",
    "up": "yarn upgrade-interactive --latest"
  },
